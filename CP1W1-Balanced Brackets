#include <bits/stdc++.h>
#include <stack>
using namespace std;

// Complete the isBalanced function below.
string isBalanced(string s) {
    stack<char> st;
    size_t len=s.length();
    for(int i=0;i<len;++i){
        if(s[i]=='('||s[i]=='{'||s[i]=='['){
            st.push(s[i]);
        }
        if(s[i]=='}'||s[i]==')'||s[i]==']'){
            if(st.empty()) return "NO";
            char t=st.top();
            st.pop();
            if((s[i]==')'&&t=='(')||(s[i]=='}'&&t=='{')||(s[i]==']'&&t=='[')) continue;
            else return "NO";
        }
    }
    return st.empty()?"YES":"NO";
}

int main()
{
    ofstream fout(getenv("OUTPUT_PATH"));

    int t;
    cin >> t;
    cin.ignore(numeric_limits<streamsize>::max(), '\n');

    for (int t_itr = 0; t_itr < t; t_itr++) {
        string s;
        getline(cin, s);

        string result = isBalanced(s);

        fout << result << "\n";
    }

    fout.close();

    return 0;
}
