class Solution {
public:
    int maxPoints(vector<vector<int>>& V) {
        if(V.size()<3) return V.size();
        int n=V.size(), rst=0;
        for(int i=0;i<n;++i){
            unordered_map<unsigned long, int> mp;
            int dup=1, mx=0;
            for(int j=i+1;j<n;++j){
                if(V[j][0]==V[i][0]&&V[j][1]==V[i][1]) ++dup;
                else{
                    int x=V[i][0]-V[j][0], y=V[i][1]-V[j][1], g=__gcd(x, y);
                    x/=g, y/=g;
                    int c=++mp[(((unsigned long)x<<32)|y)];
                    mx=max(mx, c);
                }
            }
            rst=max(rst, mx+dup);
        }
        return rst;
    }
};
